{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "description": "API for E-commerce application",
    "title": "Nexa E-commerce API"
  },
  "host": "localhost:3200",
  "paths": {
    "/api/users/signin": {
      "post": {
        "summary": "Login",
        "description": "User login to get token",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "User Credentials",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "response": {
          "200": {
            "description": "OK"
          },
          "400": {
            "description": "Incorrect Credentals"
          }
        }
      }
    }
  }
}
